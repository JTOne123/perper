<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <AzureFunctionsVersion></AzureFunctionsVersion>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Azure.WebJobs.Script.ExtensionsMetadataGenerator" Version="1.1.2" />
        <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="3.0.0-preview2" />
    </ItemGroup>
    <ItemGroup>
        <None Update="host.json">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="local.settings.json">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <CopyToPublishDirectory>Never</CopyToPublishDirectory>
        </None>
        <None Update="requirements.txt">
          <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Launcher\__init__.py">
          <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Launcher\function.json">
          <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Remove="Launcher\__pycache__\**" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\..\src\Perper.WebJobs.Extensions\Perper.WebJobs.Extensions.csproj" />
    </ItemGroup>
    <ItemGroup>
      <Compile Remove="Launcher\__pycache__\**" />
    </ItemGroup>
    <ItemGroup>
      <EmbeddedResource Remove="Launcher\__pycache__\**" />
    </ItemGroup>
</Project>